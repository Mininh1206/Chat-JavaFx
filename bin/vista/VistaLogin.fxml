      <TextField fx:id="tbIp" layoutX="58.0" layoutY="101.0" prefHeight="25.0" prefWidth="185.0" promptText="Ip" />
      <TextField fx:id="tbPuerto" layoutX="58.0" layoutY="150.0" prefHeight="25.0" prefWidth="185.0" promptText="Puerto" />
      <Button fx:id="btConectar" layoutX="58.0" layoutY="220.0" mnemonicParsing="false" onAction="#conectar" text="Conectar" />
      <Button fx:id="btCrear" layoutX="179.0" layoutY="220.0" mnemonicParsing="false" onAction="#crear" prefHeight="25.0" prefWidth="64.0" text="Crear" />
   </children>
